{
  "manifest_version": 3,
  "name": "ACExt",
  "description": "Atkascha's Chrome Extension",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["https://www.twitch.tv/*"],
      "js": ["scripts/twitch.js"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  }
}
